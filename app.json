{
  "expo": {
    "name": "Vlinx",
    "slug": "Vlinx",
    "scheme": "vlinx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ic_launcher_round.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ic_launcher.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/images/ic_launcher_round.png"
    },
    
    "plugins": [
      "expo-font",
      "expo-router"
    ]
  }
}

  

